<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<GridPane id="rootPane" alignment="CENTER" hgap="10.0" minHeight="250.0" minWidth="250.0" prefHeight="550.0" prefWidth="900.0" stylesheets="@../../css/TitleScreens.css" vgap="10.0" xmlns="http://javafx.com/javafx/8.0.76-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="agilepuppers.cleanwater.controller.TitleScreen">
        <Text fx:id="title" textAlignment="CENTER" GridPane.halignment="CENTER" GridPane.rowIndex="0">
            <!--Value gets loaded automatically in controller-->
            <font>
                <Font size="50" />
            </font>
            <GridPane.margin>
                <Insets bottom="50.0" />
            </GridPane.margin>
        </Text>
        <Button onAction="#handleLogin" prefHeight="25.0" prefWidth="95.0" GridPane.rowIndex="1" GridPane.halignment="CENTER">
            Log In
        </Button>
        <Button onAction="#handleRegister" prefHeight="25.0" prefWidth="95.0" GridPane.rowIndex="2" GridPane.halignment="CENTER">
            Register
        </Button>
</GridPane>
